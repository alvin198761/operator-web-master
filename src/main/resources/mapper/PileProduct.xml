<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jczc.operatorweb.dao.PileProductDao">
    <select id="queryAllPileProducts" resultType="com.jczc.operatorweb.entity.PileProduct">
        select
        *
        from
        charging_pile_product cpp
        where cpp.status = 1
    </select>
</mapper>